<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_custom_evaluate_fragment_first"
    android:layout_width="match_parent"
    android:layout_height="46dp"
    android:background="#E5E5E5"
    android:orientation="vertical">

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="411dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle_option" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="-50dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/admin_custom_evaluate_option_tat_ca"
            android:layout_width="120dp"
            android:layout_height="47dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:text="Tất cả"
            android:textColor="#FFFFFF"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/admin_custom_evaluate_option_chua_phan_hoi"
            android:layout_width="120dp"
            android:layout_height="47dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:text="Chưa phản hồi"
            android:textColor="#FFFFFF"
            android:textSize="14sp" />

    </LinearLayout>

</LinearLayout>